    <connection>AdventureWorks</connection>
    <sql>SELECT
pr.ProductNumber,
pr.Name,
pr.MakeFlag,
pr.FinishedGoodsFlag,
pr.Color,
pr.Size,
pr.Weight,
pr.ProductLine,
pr.Class,
pr.Style,
pc.Name as categoria,
ps.Name as subcategoria
From 
Production.Product pr
left join Production.ProductSubcategory ps ON pr.ProductSubcategoryID = ps.ProductSubcategoryID
left join Production.ProductCategory pc On ps.ProductCategoryID = pc.ProductCategoryID
WHERE
pr.FinishedGoodsFlag = 1</sql>
